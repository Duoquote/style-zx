{
  "name": "style-zx",
  "private": false,
  "version": "0.0.14",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Duoquote/style-zx.git"
  },
  "bugs": {
    "url": "https://github.com/Duoquote/style-zx/issues"
  },
  "homepage": "https://github.com/Duoquote/style-zx#readme",
  "keywords": [
    "css-in-js",
    "react",
    "zero-runtime",
    "styling",
    "css",
    "typescript",
    "zx"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build -c vite.config.lib.ts",
    "build:example": "vite build --outDir dist-example",
    "lint": "eslint .",
    "preview": "vite preview --outDir dist-example",
    "prepreview": "npm run build:example",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "lodash": "^4.17.21",
    "magic-string": "^0.30.21",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "ajv": "^8.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  }
}
